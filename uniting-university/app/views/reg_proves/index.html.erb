<p id="notice"><%= notice %></p>

<h1>Reg Proves</h1>

<table>
  <thead>
    <tr>
      <th>Nombre apellido</th>
      <th>Usuario</th>
      <th>Contrasena</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reg_proves.each do |reg_profe| %>
      <tr>
        <td><%= reg_profe.nombre_apellido %></td>
        <td><%= reg_profe.usuario %></td>
        <td><%= reg_profe.contrasena %></td>
        <td><%= reg_profe.email %></td>
        <td><%= link_to 'Show', reg_profe %></td>
        <td><%= link_to 'Edit', edit_reg_profe_path(reg_profe) %></td>
        <td><%= link_to 'Destroy', reg_profe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reg Profe', new_reg_profe_path %>
